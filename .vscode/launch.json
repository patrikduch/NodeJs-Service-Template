{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Current TS File",
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"args": ["${relativeFile}"],
			"runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
			"sourceMaps": true,
			"cwd": "${workspaceRoot}",
			"protocol": "inspector"
		}
	]
}
